#include <iostream>

using namespace std;

int main()
{
    char a;
    cout<<"\n are you educated about politics in your country?\n";
    cin>>a;

    int age;
    cout<<"enter your age\n";
    cin>>age;
    
    if(age==18)
    {
        cout<<"you are eligible for voting";
    }
    else if(age<=70)
    {
        cout<<"\nyou are eligible for voting";
    }
    else
    {
     cout<<"\n you are not eligible for voting";
     
    }
    
    

    return 0;
}
