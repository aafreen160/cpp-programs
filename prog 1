#include <iostream>

using namespace std;

int main()
{
    int a;
    char b;
    float c;
    double d;
    cout<<"size of int:"<<sizeof(a)<<"bytes";
    cout<<"\nsize of char:"<<sizeof(b)<<"bytes";
    cout<<"\nsize of float:"<<sizeof(c)<<"bytes";
    cout<<"\nsize of double:"<<sizeof(d)<<"bytes";
    return 0;
}



#include <iostream>

using namespace std;

int main()
{
    int a,b,c;
    cout<<"enter two numbers:";
    cin>>a>>b;
    c=a*b;
    cout<<"\n the product is:"<<c;
    
    return 0;
}







